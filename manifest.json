{
	"background":
	{
		"scripts": ["background.js"]
	},
	"browser_action":
	{
		"default_icon": {
			"19": "19.png",
			"38": "38.png"
		},
		"default_title": "Chater",
		"default_popup": "popup.html"
	},
	"content_scripts":
	[{
		"js": ["content.js"],
		"css": ["content.css"],
		"matches": ["*://*/*"],
		"run_at": "document_start"
	}],
	"description": "Description",
	"icons":
	{
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"manifest_version": 2,
	"name": "Chater",
	"options_page": "options.html",
	"permissions":
	[
		"*://*/*",
		"contextMenus",
		"identity",
		"notifications",
		"tabs",
		"tabCapture",
		"unlimitedStorage",
		"webRequest"
	],
	"version": "0.8.4",
	"web_accessible_resources":
	[
		"16.png",
		"48.png",
		"128.png"
	]
}